{% extends '::layout.html.twig' %}

{% block body -%}
    <h1>InscritSemin list</h1>

    <table class="table table-bordered table-striped table-condensed table-hover" style="width: 80%; margin-left: 10%;">
        <thead>
            <tr>
                <th>Id</th>
                <th>Dateinscrit</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('inscritsemin_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{% if entity.dateInscrit %}{{ entity.dateInscrit|date('Y-m-d') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('inscritsemin_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('inscritsemin_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                    {#<li>{{ iduser }}</li>#}
                    

                </ul>
                </td>
                <td>{{ etudiant }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('inscritsemin_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
